{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "..\\..\\..\\Logs\\.log",
          "rollingInterval": "Day"
        }
      }
    ],
    "MinimalLevel": {
      "Default": "Trace",
      "Override": {
        "Microsoft": "Trace",
        "System": "Trace"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
