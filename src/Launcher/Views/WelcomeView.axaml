<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converters="clr-namespace:Launcher.Converters"
             xmlns:vm="clr-namespace:Launcher.ViewModels"
             xmlns:prism="http://prismlibrary.com/"
             x:Class="Launcher.Views.WelcomeView"
             prism:ViewModelLocator.AutoWireViewModel="true">
    <UserControl.Resources>
        <converters:BoolInverterConverter x:Key="BoolInverterConverter" />
    </UserControl.Resources>
    <Design.DataContext>
        <vm:WelcomeViewModel />
    </Design.DataContext>
    
    <StackPanel Orientation="Vertical">
        <Label Classes="h1 text-primary">Lisiados</Label>
        <Label Classes="h2 text-secondary">Lisiados</Label>
        <Label Classes="h3 text-success">Lisiados</Label>
        <Label Classes="h4 text-danger">Lisiados</Label>
        <Label Classes="h5 text-warning">Lisiados</Label>
        <Label Classes="h6 text-info">Lisiados</Label>
    </StackPanel>
    
</UserControl>