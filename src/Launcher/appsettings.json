{
    "Minecraft": {
        "MinVersion": "1.14",
        "Betas": false
    },
    "Services": {
        "Downloader": {
            "Versions": "https://mrarm.io/r/w10-vdb"
        }
    },
    "Serilog": {
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": "%appdata%\\..\\Local\\Minecraft\\Logs\\.log",
                    "rollingInterval": "Day"
                }
            }
        ],
        "MinimalLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Information",
                "System": "Warning"
            }
        },
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithProcessId",
            "WithThreadId"
        ]
    },
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    }
}